# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: bootloader_M7.axf

# Tool invocations
bootloader_M7.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="bootloader_M7.map" -Xlinker --gc-sections -Xlinker -print-memory-usage -Xlinker --sort-section=alignment -Xlinker --cref -mcpu=cortex-m7 -mfpu=fpv5-d16 -mfloat-abi=hard -mthumb -T "bootloader_M7_Debug.ld" -o "bootloader_M7.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(EXECUTABLES)$(OBJS)$(C_DEPS) bootloader_M7.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "bootloader_M7.axf" ; arm-none-eabi-objcopy -v -O binary "bootloader_M7.axf" "bootloader_M7.bin" ; checksum -p MIMXRT1021xxxxx -d "bootloader_M7.bin"
	-@echo ' '

.PHONY: all clean dependents post-build

-include ../makefile.targets
